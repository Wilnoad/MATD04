#include <iostream>

using namespace std;

int main(){
  int N, i, j, aux, id, indice, inicio, meio, fim;
  bool flag;

  cin>> N;
  int planets[N];

  for(i=0; i<N; i++)
    cin>> planets[i];
  
  id=1;

  while(id != 0){
    cin>> id;
    if(id == 0)
      return 0;
    
    flag = false;
    inicio = 0, fim = N-1;

    while(inicio <= fim){
      meio = (inicio + fim) / 2;
      
      if(planets[meio] == id){
        flag = true, indice = meio;
        break;
      }
      else if(planets[meio] < id)
        inicio = meio + 1;
      else if(planets[meio] > id)
        fim = meio - 1;
    }

    if(flag)
      cout<< indice << "\n";
    else
      cout<< "Nao foi visitado ainda.\n";

  }

  return 0;
}
